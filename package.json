{
  "name": "key-world",
  "description": "Vue3 projects example",
  "author": {
    "name": "lisnote",
    "url": "https://github.com/lisnote"
  },
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "mock": "vite --mode mock --host",
    "staging": "vite --mode production --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview --host",
    "lint": "pnpm lint:stylelint && pnpm lint:prettier && pnpm lint:eslint",
    "lint:stylelint": "stylelint **/*.{css,scss,vue,html} --fix --ignore-path .gitignore",
    "lint:prettier": "prettier --write  \"./**/*.{js,ts,json,css,scss,vue,html,md}\" --ignore-path .gitignore",
    "lint:eslint": "eslint  \"./**/{\\.*,*}.{vue,js,ts}\" --fix --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^9.6.0",
    "axios": "^1.1.3",
    "element-plus": "^2.3.7",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@iconify/vue": "^4.1.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.8",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.1",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "prettier": "^3.1.0",
    "sass": "^1.55.0",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.1.6",
    "unocss": "^0.57.7",
    "vconsole": "^3.15.1",
    "vite": "^3.2.7",
    "vite-plugin-html": "^3.2.0",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.8.10"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}
